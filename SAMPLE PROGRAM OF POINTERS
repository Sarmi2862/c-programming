#include<stdio.h>
int main()
{
    int a=10;
    int *p=&a;
    printf("\n the values of a is:%d",a);
    printf("\n the address of a is:%d",p);
    printf("\n the address of a is:%d",&a);
    printf("\n the address of a is:%d",&p);
    printf("\n the values of a is:%d",*p);
    printf("\n the values of a is:%d",((&p)));
return 0;
}
